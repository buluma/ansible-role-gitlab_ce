---
#
# Ansible managed
#
exclude_paths:
  - ./meta/preferences.yml
  - ./molecule/default/prepare.yml
  - ./molecule/default/converge.yml
  - ./molecule/default/verify.yml
  - ./molecule/default/collections.yml
  - ./molecule/default/cleanup.yml
  - ./.tox
  - ./.cache
  - ./.github
  - ./requirements.yml

warn_list:
  - parser-error
  - experimental
  - schema[meta]

skip_list:
  - experimental
